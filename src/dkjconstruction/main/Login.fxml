            <Font name="Raleway-Regular" size="12.0" />
         </font></TextField>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="150.0" layoutY="35.0" prefHeight="68.0" prefWidth="301.0" text="DKJ CONSTRUCTION" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Raleway-ExtraBold" size="30.0" />
